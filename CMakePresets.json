{
    "version": 3,
    "configurePresets": [
        {
            "name": "default",
            "hidden": true,
            "generator": "Ninja",
            "binaryDir": "${sourceDir}/build/${presetName}",
            "toolchainFile": "${sourceDir}/cmake/gnu-tools-for-stm32.cmake",
            "cacheVariables": {
                "CMAKE_EXPORT_COMPILE_COMMANDS": "ON"
            }
        },
        {
            "name": "stm32-debug",
            "inherits": "default",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Debug",

                "PROJECT_ORIGINAL_NAME": "test32",

                "CMSIS_Dvendor": "STMicroelectronics:13",
                "CMSIS_Dname": "STM32F103C8T6",
                "CMSIS_Dcore": "Cortex-M3",
                "CMSIS_Dtz": "NO_TZ",
                "CMSIS_Dfpu": "NO_FPU",
                "CMSIS_Ddsp": "NO_DSP",
                "CMSIS_Dmve": "NO_MVE",
                "CMSIS_Dmpu": "MPU",
                "CMSIS_Dendian": "Little-endian",
                "CMSIS_Dsecure": "",
                "CMSIS_Tcompiler": "GCC"
            }
        },
        {
            "name": "stm32-release",
            "inherits": "default",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Release",

                "PROJECT_ORIGINAL_NAME": "test32",

                "CMSIS_Dvendor": "STMicroelectronics:13",
                "CMSIS_Dname": "STM32F103C8T6",
                "CMSIS_Dcore": "Cortex-M3",
                "CMSIS_Dtz": "NO_TZ",
                "CMSIS_Dfpu": "NO_FPU",
                "CMSIS_Ddsp": "NO_DSP",
                "CMSIS_Dmve": "NO_MVE",
                "CMSIS_Dmpu": "MPU",
                "CMSIS_Dendian": "Little-endian",
                "CMSIS_Dsecure": "",
                "CMSIS_Tcompiler": "GCC"
            }
        }
    ],
    "buildPresets": [
        {
            "name": "default",
            "hidden": true,
            "configurePreset": "default"
        },
                {
            "name": "stm32-debug",
            "configurePreset": "stm32-debug"
        },
        {
            "name": "stm32-release",
            "configurePreset": "stm32-release"
        }
    ]
}
